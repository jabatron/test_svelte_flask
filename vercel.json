{
  "version": 2,
  "projects": [
    {
      "src": "f/",
      "use": "@vercel/static-build",
      "buildCommand": "npm run build",
      "outputDirectory": "build",
      "routes": [
        {
          "src": "/(.*)",
          "dest": "/f/$1"
        }
      ]
    },
    {
      "src": "b/",
      "use": "@vercel/python",
      "routes": [
        {
          "src": "/api/(.*)",
          "dest": "/backend/api/$1"
        }
      ]
    }
  ]
}