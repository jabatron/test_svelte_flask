
{
"version": 2,
"builds": [
  {
    "src": "f/src",
    "dest": ".",
    "command": "npm run build" 
  },
  {
    "src": "b/app.py",
    "dest": ".",
    "use": "@vercel/python"
  }
],
"routes":   [
  {
    "src": "/(.*)",
    "dest": "/f/dist"
  },
  {
    "src": "/api/(.*)",
    "dest": "/b/app.py"
  }
]
}